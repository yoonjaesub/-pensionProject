<mapper namespace="RoomMapper">
	<!-- 객실 추가 기능 -->
	<insert id="insert">
		insert into room_tbl(
								r_number, 
								r_name, 
								r_price, 
								r_description, 
								r_guestsnum )
						 values (#{r_number}, 
						 		#{r_name}, 
						 		#{r_price}, 
						 		#{r_desciption}, 
						 		#{r_guestsnum} )
	</insert>
</mapper>